{
    "name": "sofyco/jwt-authenticator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JWT Authenticator Bundle",
    "keywords": ["php", "symfony", "bundle", "security", "jwt", "authenticator"],
    "homepage": "https://github.com/sofyco/jwt-authenticator-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiia Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "lcobucci/jwt": "^5.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/security-bundle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "symfony/browser-kit": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\JwtAuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\JwtAuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
