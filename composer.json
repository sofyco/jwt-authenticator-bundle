{
    "name": "sofyco/jwt-authenticator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JWT Authenticator Bundle",
    "keywords": ["php", "symfony", "bundle", "security", "jwt", "authenticator"],
    "homepage": "https://github.com/sofyco/jwt-authenticator-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "lcobucci/jwt": "^4.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/messenger": "^6.2",
        "symfony/security-bundle": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\JwtAuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\JwtAuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
